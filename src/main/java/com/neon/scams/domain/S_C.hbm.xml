<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
 
<hibernate-mapping package="com.neon.scams.domain">
		<class name="S_C" table="s_c">
			<id name="id">
				<generator class="native"></generator>
			</id>
			<property name="core"></property>
			<property name="term"></property>
			
			<many-to-one name="student" column="studentid"/>
			<many-to-one name="course" column="courseid"/>

	</class>

</hibernate-mapping>
