<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
 
<hibernate-mapping package="com.neon.scams.domain">
		<class name="Innovate" table="innovate">
			<id name="id">
				<generator class="native"></generator>
			</id>
			<property name="icon"></property>
			<property name="iday"></property>
			<property name="iscore"></property>
			<property name="iterm"></property>
			
			<many-to-one name="student" column="studentid"/>

	</class>

</hibernate-mapping>
