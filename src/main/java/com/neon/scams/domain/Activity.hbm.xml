<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
 
<hibernate-mapping package="com.neon.scams.domain">
		<class name="Activity" table="activity">
			<id name="id">
				<generator class="native"></generator>
			</id>
			<property name="acon"></property>
			<property name="aday"></property>
			<property name="ascore"></property>
			<property name="aterm"></property>
			
			<many-to-one name="student" column="studentid"/>

	</class>

</hibernate-mapping>
